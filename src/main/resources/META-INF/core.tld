		<attribute>
			<name>manage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>附件上传</description>
		<name>attachUploader</name>
		<tag-class>works.tonny.apps.core.action.AttachUploadTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>name</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
