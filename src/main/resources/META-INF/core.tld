		<name>authCheck</name>
		<tag-class>works.tonny.apps.user.web.AuthCheck</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>auth</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>role</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
		<attribute>
			<name>errorMessage</name>
			<required>false</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>菜单</description>
		<name>menu</name>
		<tag-class>works.tonny.apps.user.web.MenuTag</tag-class>
		<body-content>jsp</body-content>
		<attribute>
			<name>parentId</name>
			<required>true</required>
			<rtexprvalue>true</rtexprvalue>
		</attribute>
	</tag>
	<tag>
		<description>会员菜单</description>
		<name>memberMenu</name>
