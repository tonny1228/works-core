<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC 
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="works.tonny.apps.core">
	<class name="Attachment" table="s_attachment">

		<id name="id" type="java.lang.String" column="id">
			<generator class="assigned"></generator>
		</id>
		<version name="updateTime" type="java.sql.Timestamp" column="update_time" />
		<property name="catalog" />
		<property name="title" />
		<property name="info" />
		<property name="filename" />
		<property name="filesize" type="java.lang.Integer" />
		<property name="mimetype" />
		<property name="fileext" />
		<property name="path" />
		<property name="admin" />
	</class>
</hibernate-mapping>
